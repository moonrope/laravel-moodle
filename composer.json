{
  "name": "moonrope/laravelmood",
  "description": "Laravel Moodle Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Miguel Aguirre",
      "email": "miguel@moonrope.com",
      "homepage": "https://moonrope.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "guzzlehttp/guzzle": "^7.0",
    "beberlei/assert": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "moonrope\\LaravelMoodle\\LaravelMoodleServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Moonrope\\LaravelMoodle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Moonrope\\LaravelMoodle\\Tests\\": "tests/"
    }
  }
}
