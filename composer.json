{
    "name": "qlick/laravel-moodle",
    "description": "Laravel Moodle Client",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Nermin Elkasovic",
            "email": "nermin@qlick.ch",
            "homepage": "https://qlick.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "beberlei/assert": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhiru\\LaravelMoodle\\LaravelMoodleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Zhiru\\LaravelMoodle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhiru\\LaravelMoodle\\Tests\\": "tests/"
        }
    }
}
